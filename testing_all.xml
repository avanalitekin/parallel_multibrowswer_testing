<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="WebOrders Automation Regression Tests" verbose="2">
	<listeners>
		<listener class-name="com.cbt.reports.TestListener"/>
	</listeners>
	<test name="All tests">
		<parameter name="xmlbrowser" value="chrome"></parameter>
		<packages>
			<package name="com.cbt.tests.*"></package>
		</packages>
	</test>

</suite>